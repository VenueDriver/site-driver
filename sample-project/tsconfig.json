{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": false,
    "types" : [ "node" ],
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
      "node_modules",
      "./**/node_modules",
      "./**/demo-site"
  ]
}
