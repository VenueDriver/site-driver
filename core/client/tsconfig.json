{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015"],
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "types": ["node"],
    "typeRoots": [
      "node_modules/@types"
     ]
  },
  "exclude": [
      "node_modules",
      "./**/node_modules",
      "front"
  ]
}
