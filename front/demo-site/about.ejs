<%
let fixedNav = (layout.about_layout.sections[0] !== 'hero-slider')  ? 'fixed-nav' : '';
%>
<!doctype html>
<html lang="es">
	<%- include('includes/head') %>
<body class="<%= fixedNav %>">
	<%- include('includes/header') %>
	<main>
		<% layout.about_layout.sections.forEach(section =>{ %>
			<% if(section === "generic-section"){ %>
				<%- include('section-templates/'+section , about.generic_section) %>
			<% }else{ %>
				<%- include('section-templates/'+section) %>
			<% } %>
		<% }) %>
	</main>
	<%- include('includes/footer') %>
	<!--JQUERY -->
	<script type="text/javascript" src="https://s3-us-west-1.amazonaws.com/assets.portaldriver.com/demo-site/demo-site/js/jquery-1.11.0.min.js"></script>
	<script type="text/javascript" src="https://s3-us-west-1.amazonaws.com/assets.portaldriver.com/demo-site/demo-site/js/owl.carousel.min.js"></script>
	<script type="text/javascript" src="https://s3-us-west-1.amazonaws.com/assets.portaldriver.com/demo-site/demo-site/js/lightgallery.min.js"></script>
	<script type="text/javascript" src="https://s3-us-west-1.amazonaws.com/assets.portaldriver.com/demo-site/demo-site/js/main.js"></script>
</body>
</html>
